    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {},
        "[One Dark Pro]": {
            "comments": "#339999"
        },
        "[Dracula]": {
            "comments": "#7A8ECC",
            "functions": "#F1B868",
            "textMateRules": [
                {
                    "scope": "entity.name.smartlang",
                    "settings": {}
                },
                {
                    "scope": "keyword.operator.arithmetic.smartlang",
                    "settings": {
                        "foreground": "#CCCC33"
                    }
                },
                {
                    "scope": "constant.numeric.smartlang",
                    "settings": {
                        "foreground": "#00FFFF"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": [
        "Launch Client"
    ]
}